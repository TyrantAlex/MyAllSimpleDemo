<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="tab_count" format="integer" />
    <attr name="text_color_normal" format="color" />
    <attr name="text_color_selected" format="color" />
    <attr name="text_size" format="dimension" />
    <attr name="indicator_color" format="color" />
    <attr name="indicator_style">
        <enum name="line" value="1" />
    </attr>

    <declare-styleable name="ViewPagerIndicator">
        <attr name="tab_count" />
        <attr name="text_color_normal" />
        <attr name="text_color_selected" />
        <attr name="text_size" />
        <attr name="indicator_color" />
        <attr name="indicator_style" />
    </declare-styleable>
</resources>
